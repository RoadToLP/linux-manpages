.SH NAME
amdgpu_acpi_is_pcie_performance_request_supported \- 
.SH SYNOPSIS
.B "bool" amdgpu_acpi_is_pcie_performance_request_supported
.BI "(struct amdgpu_device *adev "  ");"
.SH ARGUMENTS
.IP "adev" 12
