.TH "v9fs_vfs_mkdir" 9 "v9fs_vfs_mkdir" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
v9fs_vfs_mkdir \- VFS mkdir hook to create a directory
.SH SYNOPSIS
.B "int" v9fs_vfs_mkdir
.BI "(struct user_namespace *mnt_userns "  ","
.BI "struct inode *dir "  ","
.BI "struct dentry *dentry "  ","
.BI "umode_t mode "  ");"
.SH ARGUMENTS
.IP "mnt_userns" 12
-- undescribed --
.IP "dir" 12
inode that is being unlinked
.IP "dentry" 12
dentry that is being unlinked
.IP "mode" 12
mode for new directory
