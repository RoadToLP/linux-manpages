.SH SYNOPSIS
.B "void" platform_msi_domain_free
.BI "(struct irq_domain *domain "  ","
.BI "unsigned int virq "  ","
.BI "unsigned int nvec "  ");"
.SH ARGUMENTS
.IP "domain" 12
