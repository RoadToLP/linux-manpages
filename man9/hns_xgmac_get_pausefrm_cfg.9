.TH "hns_xgmac_get_pausefrm_cfg" 9 "hns_xgmac_get_pausefrm_cfg" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
hns_xgmac_get_pausefrm_cfg \- get xgmac pause param
.SH SYNOPSIS
.B "void" hns_xgmac_get_pausefrm_cfg
.BI "(void *mac_drv "  ","
.BI "u32 *rx_en "  ","
.BI "u32 *tx_en "  ");"
.SH ARGUMENTS
.IP "mac_drv" 12
mac driver
.IP "rx_en" 12
xgmac rx pause enable
.IP "tx_en" 12
xgmac tx pause enable
