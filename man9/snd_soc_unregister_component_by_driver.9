.SH SYNOPSIS
.B "void" snd_soc_unregister_component_by_driver
.BI "(struct device *dev "  ","
.BI "const struct snd_soc_component_driver *component_driver "  ");"
.SH ARGUMENTS
.IP "dev" 12
