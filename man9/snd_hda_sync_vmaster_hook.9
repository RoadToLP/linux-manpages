.SH NAME
snd_hda_sync_vmaster_hook \- Sync vmaster hook
.SH SYNOPSIS
.B "void" snd_hda_sync_vmaster_hook
.BI "(struct hda_vmaster_mute_hook *hook "  ");"
.SH ARGUMENTS
.IP "hook" 12
the vmaster hook
.SH "DESCRIPTION"
