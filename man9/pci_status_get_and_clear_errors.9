.SH NAME
pci_status_get_and_clear_errors \- return and clear error bits in PCI_STATUS
.SH SYNOPSIS
.B "int" pci_status_get_and_clear_errors
.BI "(struct pci_dev *pdev "  ");"
.SH ARGUMENTS
.IP "pdev" 12
