.SH SYNOPSIS
.B "int" tps65217_update_bits
.BI "(struct tps65217 *tps "  ","
.BI "unsigned int reg "  ","
.BI "unsigned int mask "  ","
.BI "unsigned int val "  ","
.BI "unsigned int level "  ");"
