.TH "batadv_check_management_packet" 9 "batadv_check_management_packet" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
batadv_check_management_packet \- Check preconditions for management packets
.SH SYNOPSIS
.B "bool" batadv_check_management_packet
.BI "(struct sk_buff *skb "  ","
.BI "struct batadv_hard_iface *hard_iface "  ","
.BI "int header_len "  ");"
.SH ARGUMENTS
.IP "skb" 12
incoming packet buffer
.IP "hard_iface" 12
incoming hard interface
.IP "header_len" 12
minimal header length of packet type
.SH "RETURN"
true when management preconditions are met, false otherwise
