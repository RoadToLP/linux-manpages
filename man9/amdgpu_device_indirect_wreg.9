.B "void" amdgpu_device_indirect_wreg
.BI "(struct amdgpu_device *adev "  ","
.BI "u32 pcie_index "  ","
.BI "u32 pcie_data "  ","
.BI "u32 reg_addr "  ","
.BI "u32 reg_data "  ");"
.SH ARGUMENTS
.IP "adev" 12
amdgpu_device pointer
