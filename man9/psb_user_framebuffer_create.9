.SH NAME
psb_user_framebuffer_create \- create framebuffer
.SH SYNOPSIS
.B "struct drm_framebuffer *" psb_user_framebuffer_create
.BI "(struct drm_device *dev "  ","
.BI "struct drm_file *filp "  ","
.BI "const struct drm_mode_fb_cmd2 *cmd "  ");"
