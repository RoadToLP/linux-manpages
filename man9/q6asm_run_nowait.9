.SH SYNOPSIS
.B "int" q6asm_run_nowait
.BI "(struct audio_client *ac "  ","
.BI "uint32_t stream_id "  ","
.BI "uint32_t flags "  ","
.BI "uint32_t msw_ts "  ","
.BI "uint32_t lsw_ts "  ");"
.SH ARGUMENTS
.IP "ac" 12
audio client pointer
.IP "stream_id" 12
stream id
.IP "flags" 12
flags associated with write
.IP "msw_ts" 12
timestamp msw
.IP "lsw_ts" 12
timestamp lsw
.SH "RETURN"
