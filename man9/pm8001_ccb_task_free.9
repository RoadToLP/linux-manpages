.SH SYNOPSIS
.B "void" pm8001_ccb_task_free
.BI "(struct pm8001_hba_info *pm8001_ha "  ","
.BI "struct sas_task *task "  ","
.BI "struct pm8001_ccb_info *ccb "  ","
.BI "u32 ccb_idx "  ");"
.SH ARGUMENTS
.IP "pm8001_ha" 12
our hba card information
.IP "task" 12
the task to be free.
.IP "ccb" 12
the ccb which attached to ssp task
.IP "ccb_idx" 12
ccb index.
