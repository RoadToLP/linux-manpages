.TH "i40iw_sc_qp_setctx" 9 "i40iw_sc_qp_setctx" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
i40iw_sc_qp_setctx \- set qp's context
.SH SYNOPSIS
.B "enum i40iw_status_code" i40iw_sc_qp_setctx
.BI "(struct i40iw_sc_qp *qp "  ","
.BI "u64 *qp_ctx "  ","
.BI "struct i40iw_qp_host_ctx_info *info "  ");"
.SH ARGUMENTS
.IP "qp" 12
sc qp
.IP "qp_ctx" 12
context ptr
.IP "info" 12
ctx info
