.SH NAME
igb_enable_mng_pass_thru \- Enable processing of ARP's
.SH SYNOPSIS
.B "bool" igb_enable_mng_pass_thru
.BI "(struct e1000_hw *hw "  ");"
.SH ARGUMENTS
