.SH NAME
i40iw_addr_resolve_neigh \- resolve neighbor address
.SH SYNOPSIS
.B "int" i40iw_addr_resolve_neigh
.BI "(struct i40iw_device *iwdev "  ","
.BI "u32 src_ip "  ","
.BI "u32 dst_ip "  ","
.BI "int arpindex "  ");"
.SH ARGUMENTS
.IP "iwdev" 12
iwarp device structure
.IP "src_ip" 12
local ip address
.IP "dst_ip" 12
remote ip address
.IP "arpindex" 12
if there is an arp entry
