.TH "ixgbe_ptp_adjtime" 9 "ixgbe_ptp_adjtime" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
ixgbe_ptp_adjtime \- 
.SH SYNOPSIS
.B "int" ixgbe_ptp_adjtime
.BI "(struct ptp_clock_info *ptp "  ","
.BI "s64 delta "  ");"
.SH ARGUMENTS
.IP "ptp" 12
