.SH NAME
amdgpu_vce_cs_reloc \- command submission relocation
.SH SYNOPSIS
.B "int" amdgpu_vce_cs_reloc
.BI "(struct amdgpu_cs_parser *p "  ","
.BI "uint32_t ib_idx "  ","
.BI "int lo "  ","
.BI "int hi "  ","
.BI "unsigned size "  ","
.BI "uint32_t index "  ");"
.SH ARGUMENTS
.IP "p" 12
parser context
.IP "ib_idx" 12
