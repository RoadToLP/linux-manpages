.TH "ide_pci_dma_base" 9 "ide_pci_dma_base" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
ide_pci_dma_base \- setup BMIBA
.SH SYNOPSIS
.B "unsigned long" ide_pci_dma_base
.BI "(ide_hwif_t *hwif "  ","
.BI "const struct ide_port_info *d "  ");"
.SH ARGUMENTS
.IP "hwif" 12
