.TH "nilfs_cpfile_read" 9 "nilfs_cpfile_read" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
nilfs_cpfile_read \- read or get cpfile inode
.SH SYNOPSIS
.B "int" nilfs_cpfile_read
.BI "(struct super_block *sb "  ","
.BI "size_t cpsize "  ","
.BI "struct nilfs_inode *raw_inode "  ","
.BI "struct inode **inodep "  ");"
.SH ARGUMENTS
.IP "sb" 12
super block instance
.IP "cpsize" 12
size of a checkpoint entry
.IP "raw_inode" 12
