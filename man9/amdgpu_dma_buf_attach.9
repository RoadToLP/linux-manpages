.TH "amdgpu_dma_buf_attach" 9 "amdgpu_dma_buf_attach" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
amdgpu_dma_buf_attach \- &dma_buf_ops.attach implementation
.SH SYNOPSIS
.B "int" amdgpu_dma_buf_attach
.BI "(struct dma_buf *dmabuf "  ","
.BI "struct dma_buf_attachment *attach "  ");"
