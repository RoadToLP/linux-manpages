.TH "ecryptfs_set_default_crypt_stat_vals" 9 "ecryptfs_set_default_crypt_stat_vals" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
ecryptfs_set_default_crypt_stat_vals \- 
.SH SYNOPSIS
.B "void" ecryptfs_set_default_crypt_stat_vals
.BI "(struct ecryptfs_crypt_stat *crypt_stat "  ","
.BI "struct ecryptfs_mount_crypt_stat *mount_crypt_stat "  ");"
.SH ARGUMENTS
.IP "crypt_stat" 12
The inode's cryptographic context
.IP "mount_crypt_stat" 12
The mount point's cryptographic context
.SH "DESCRIPTION"
