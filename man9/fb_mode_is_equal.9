.TH "fb_mode_is_equal" 9 "fb_mode_is_equal" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
fb_mode_is_equal \- compare 2 videomodes
.SH SYNOPSIS
.B "int" fb_mode_is_equal
.BI "(const struct fb_videomode *mode1 "  ","
.BI "const struct fb_videomode *mode2 "  ");"
.SH ARGUMENTS
.IP "mode1" 12
first videomode
.IP "mode2" 12
second videomode
.SH "RETURN"
1 if equal, 0 if not
