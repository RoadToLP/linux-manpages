.SH NAME
ice_dcbnl_set_pg_tc_cfg_tx \- set CEE PG Tx config
.SH SYNOPSIS
.B "void" ice_dcbnl_set_pg_tc_cfg_tx
.BI "(struct net_device *netdev "  ","
.BI "int tc "  ","
.BI "u8 __always_unused prio_type "  ","
.BI "u8 __always_unused bwg_id "  ","
.BI "u8 __always_unused bw_pct "  ","
.BI "u8 up_map "  ");"
.SH ARGUMENTS
.IP "netdev" 12
pointer to relevant netdev
.IP "tc" 12
the corresponding traffic class
.IP "prio_type" 12
the traffic priority type
.IP "bwg_id" 12
the BW group ID the TC belongs to
.IP "bw_pct" 12
the BW perventage for the BWG
.IP "up_map" 12
