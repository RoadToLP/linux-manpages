.SH SYNOPSIS
.B "int" i2c_arbitrator_deselect
.BI "(struct i2c_mux_core *muxc "  ","
.BI "u32 chan "  ");"
.SH ARGUMENTS
.IP "muxc" 12
-- undescribed --
