.TH "hns_dsaf_tcam_mc_cfg" 9 "hns_dsaf_tcam_mc_cfg" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
hns_dsaf_tcam_mc_cfg \- cfg the tcam for mc
.SH SYNOPSIS
.B "void" hns_dsaf_tcam_mc_cfg
.BI "(struct dsaf_device *dsaf_dev "  ","
.BI "u32 address "  ","
.BI "struct dsaf_tbl_tcam_data *ptbl_tcam_data "  ","
.BI "struct dsaf_tbl_tcam_data *ptbl_tcam_mask "  ","
.BI "struct dsaf_tbl_tcam_mcast_cfg *ptbl_tcam_mcast "  ");"
.SH ARGUMENTS
.IP "dsaf_dev" 12
dsa fabric device struct pointer
.IP "address" 12
tcam index
.IP "ptbl_tcam_data" 12
tcam data struct pointer
.IP "ptbl_tcam_mask" 12
tcam mask struct pointer, it must be null for HNSv1
.IP "ptbl_tcam_mcast" 12
tcam data struct pointer
