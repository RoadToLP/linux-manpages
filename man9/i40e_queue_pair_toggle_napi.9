.SH SYNOPSIS
.B "void" i40e_queue_pair_toggle_napi
.BI "(struct i40e_vsi *vsi "  ","
.BI "int queue_pair "  ","
.BI "bool enable "  ");"
.SH ARGUMENTS
.IP "vsi" 12
vsi
.IP "queue_pair" 12
queue pair
.IP "enable" 12
true for enable, false for disable
