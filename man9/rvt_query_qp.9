.TH "rvt_query_qp" 9 "rvt_query_qp" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
rvt_query_qp \- query an ipbq
.SH SYNOPSIS
.B "int" rvt_query_qp
.BI "(struct ib_qp *ibqp "  ","
.BI "struct ib_qp_attr *attr "  ","
.BI "int attr_mask "  ","
.BI "struct ib_qp_init_attr *init_attr "  ");"
.SH ARGUMENTS
.IP "ibqp" 12
IB qp to query
.IP "attr" 12
attr struct to fill in
.IP "attr_mask" 12
attr mask ignored
.IP "init_attr" 12
struct to fill in
.SH "RETURN"
always 0
