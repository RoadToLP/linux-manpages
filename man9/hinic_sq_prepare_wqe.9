.BI "(struct hinic_sq *sq "  ","
.BI "u16 prod_idx "  ","
.BI "struct hinic_sq_wqe *sq_wqe "  ","
.BI "struct hinic_sge *sges "  ","
.BI "int nr_sges "  ");"
.SH ARGUMENTS
.IP "sq" 12
send queue
.IP "prod_idx" 12
pi value
.IP "sq_wqe" 12
wqe to prepare
