.B "i40e_status" i40e_aq_get_firmware_version
.BI "(struct i40e_hw *hw "  ","
.BI "u16 *fw_major_version "  ","
.BI "u16 *fw_minor_version "  ","
.BI "u32 *fw_build "  ","
.BI "u16 *api_major_version "  ","
.BI "u16 *api_minor_version "  ","
.BI "struct i40e_asq_cmd_details *cmd_details "  ");"
.SH ARGUMENTS
.IP "hw" 12
pointer to the hw struct
.IP "fw_major_version" 12
firmware major version
.IP "fw_minor_version" 12
firmware minor version
