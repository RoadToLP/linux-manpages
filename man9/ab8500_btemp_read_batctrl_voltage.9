.SH NAME
ab8500_btemp_read_batctrl_voltage \- measure batctrl voltage
.SH SYNOPSIS
.B "int" ab8500_btemp_read_batctrl_voltage
.BI "(struct ab8500_btemp *di "  ");"
.SH ARGUMENTS
.IP "di" 12
pointer to the ab8500_btemp structure
.SH "DESCRIPTION"
