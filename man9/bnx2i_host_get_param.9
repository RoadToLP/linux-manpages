.B "int" bnx2i_host_get_param
.BI "(struct Scsi_Host *shost "  ","
.BI "enum iscsi_host_param param "  ","
.BI "char *buf "  ");"
.SH ARGUMENTS
.IP "shost" 12
scsi host pointer
.IP "param" 12
parameter type identifier
.IP "buf" 12
buffer pointer
