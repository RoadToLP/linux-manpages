.SH NAME
v9fs_vfs_mknod \- create a special file
.SH SYNOPSIS
.B "int" v9fs_vfs_mknod
.BI "(struct user_namespace *mnt_userns "  ","
.BI "struct inode *dir "  ","
.BI "struct dentry *dentry "  ","
.BI "umode_t mode "  ","
.BI "dev_t rdev "  ");"
.SH ARGUMENTS
.IP "mnt_userns" 12
-- undescribed --
.IP "dir" 12
inode destination for new link
.IP "dentry" 12
dentry for file
.IP "mode" 12
mode for creation
.IP "rdev" 12
device associated with special file
