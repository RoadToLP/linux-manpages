.SH NAME
tipc_media_addr_printf \- record media address in print buffer
.SH SYNOPSIS
.B "int" tipc_media_addr_printf
.BI "(char *buf "  ","
.BI "int len "  ","
.BI "struct tipc_media_addr *a "  ");"
.SH ARGUMENTS
.IP "buf" 12
output buffer
.IP "len" 12
output buffer size remaining
.IP "a" 12
input media address
