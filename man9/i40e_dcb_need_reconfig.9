.SH NAME
i40e_dcb_need_reconfig \- Check if DCB needs reconfig
.SH SYNOPSIS
.B "bool" i40e_dcb_need_reconfig
.BI "(struct i40e_pf *pf "  ","
.BI "struct i40e_dcbx_config *old_cfg "  ","
.BI "struct i40e_dcbx_config *new_cfg "  ");"
.SH ARGUMENTS
.IP "pf" 12
board private structure
.IP "old_cfg" 12
current DCB config
.IP "new_cfg" 12
new DCB config
