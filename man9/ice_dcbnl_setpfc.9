.B "int" ice_dcbnl_setpfc
.BI "(struct net_device *netdev "  ","
.BI "struct ieee_pfc *pfc "  ");"
.SH ARGUMENTS
.IP "netdev" 12
pointer to relevant netdev
.IP "pfc" 12
pointer to struct holding PFC config
