.B "void" fw_caps_to_lmm
.BI "(enum fw_port_type port_type "  ","
.BI "fw_port_cap32_t fw_caps "  ","
.BI "unsigned long *link_mode_mask "  ");"
.SH ARGUMENTS
.IP "port_type" 12
Firmware Port Type
.IP "fw_caps" 12
Firmware Port Capabilities
