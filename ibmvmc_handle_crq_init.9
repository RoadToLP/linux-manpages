.SH NAME
ibmvmc_handle_crq_init \- Handle CRQ Init
.SH SYNOPSIS
.B "void" ibmvmc_handle_crq_init
.BI "(struct ibmvmc_crq_msg *crq "  ","
.BI "struct crq_server_adapter *adapter "  ");"
.SH ARGUMENTS
.IP "crq" 12
ibmvmc_crq_msg struct
.IP "adapter" 12
crq_server_adapter struct
.SH "DESCRIPTION"
