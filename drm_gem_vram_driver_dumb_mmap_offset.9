.SH SYNOPSIS
.B "int" drm_gem_vram_driver_dumb_mmap_offset
.BI "(struct drm_file *file "  ","
.BI "struct drm_device *dev "  ","
.BI "uint32_t handle "  ","
.BI "uint64_t *offset "  ");"
