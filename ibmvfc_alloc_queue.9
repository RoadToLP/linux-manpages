.TH "ibmvfc_alloc_queue" 9 "ibmvfc_alloc_queue" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
ibmvfc_alloc_queue \- Allocate queue
.SH SYNOPSIS
.B "int" ibmvfc_alloc_queue
.BI "(struct ibmvfc_host *vhost "  ","
.BI "struct ibmvfc_queue *queue "  ","
.BI "enum ibmvfc_msg_fmt fmt "  ");"
.SH ARGUMENTS
.IP "vhost" 12
ibmvfc host struct
.IP "queue" 12
ibmvfc queue to allocate
.IP "fmt" 12
queue format to allocate
.SH "RETURN"
0 on success / non-zero on failure
