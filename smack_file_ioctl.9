.SH NAME
smack_file_ioctl \- Smack check on ioctls
.SH SYNOPSIS
.B "int" smack_file_ioctl
.BI "(struct file *file "  ","
.BI "unsigned int cmd "  ","
.BI "unsigned long arg "  ");"
.SH ARGUMENTS
.IP "file" 12
