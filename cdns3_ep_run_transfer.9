.B "int" cdns3_ep_run_transfer
.BI "(struct cdns3_endpoint *priv_ep "  ","
.BI "struct usb_request *request "  ");"
.SH ARGUMENTS
.IP "priv_ep" 12
endpoint object
.IP "request" 12
request object
.SH "DESCRIPTION"
