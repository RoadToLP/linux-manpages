.SH NAME
ab8500_btemp_get_batctrl_res \- get battery resistance
.SH SYNOPSIS
.B "int" ab8500_btemp_get_batctrl_res
.BI "(struct ab8500_btemp *di "  ");"
.SH ARGUMENTS
.IP "di" 12
