.TH "lbs_debugfs_write" 9 "lbs_debugfs_write" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
lbs_debugfs_write \- proc write function
.SH SYNOPSIS
.B "ssize_t" lbs_debugfs_write
.BI "(struct file *f "  ","
.BI "const char __user *buf "  ","
.BI "size_t cnt "  ","
.BI "loff_t *ppos "  ");"
.SH ARGUMENTS
.IP "f" 12
file pointer
.IP "buf" 12
pointer to data buffer
.IP "cnt" 12
data number to write
.IP "ppos" 12
file position
.SH "RETURN"
amount of data written
