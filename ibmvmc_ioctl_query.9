.TH "ibmvmc_ioctl_query" 9 "ibmvmc_ioctl_query" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
ibmvmc_ioctl_query \- IOCTL Query
.SH SYNOPSIS
.B "long" ibmvmc_ioctl_query
.BI "(struct ibmvmc_file_session *session "  ","
.BI "struct ibmvmc_query_struct __user *ret_struct "  ");"
.SH ARGUMENTS
.IP "session" 12
ibmvmc_file_session struct
.IP "ret_struct" 12
ibmvmc_query_struct
.SH "RETURN"
0 - Success
Non-zero - Failure
