.TH "i40e_aq_config_vsi_bw_limit" 9 "i40e_aq_config_vsi_bw_limit" "May 2021" "Kernel Hacker's Manual" LINUX
.SH NAME
i40e_aq_config_vsi_bw_limit \- Configure VSI BW Limit
.SH SYNOPSIS
.B "i40e_status" i40e_aq_config_vsi_bw_limit
.BI "(struct i40e_hw *hw "  ","
.BI "u16 seid "  ","
.BI "u16 credit "  ","
.BI "u8 max_credit "  ","
.BI "struct i40e_asq_cmd_details *cmd_details "  ");"
.SH ARGUMENTS
.IP "hw" 12
pointer to the hw struct
.IP "seid" 12
VSI seid
.IP "credit" 12
BW limit credits (0 = disabled)
.IP "max_credit" 12
Max BW limit credits
.IP "cmd_details" 12
pointer to command details structure or NULL
