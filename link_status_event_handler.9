.SH NAME
link_status_event_handler \- link event handler
.SH SYNOPSIS
.B "void" link_status_event_handler
.BI "(void *handle "  ","
.BI "void *buf_in "  ","
.BI "u16 in_size "  ","
.BI "void *buf_out "  ","
.BI "u16 *out_size "  ");"
.SH ARGUMENTS
.IP "handle" 12
nic device for the handler
.IP "buf_in" 12
input buffer
.IP "in_size" 12
input size
.IP "buf_out" 12
output buffer
