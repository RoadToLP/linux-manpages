.B "int" zfcp_qdio_sbals_from_sg
.BI "(struct zfcp_qdio *qdio "  ","
.BI "struct zfcp_qdio_req *q_req "  ","
.BI "struct scatterlist *sg "  ");"
.SH ARGUMENTS
.IP "qdio" 12
pointer to struct zfcp_qdio
.IP "q_req" 12
pointer to struct zfcp_qdio_req
