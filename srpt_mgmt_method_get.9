.B "void" srpt_mgmt_method_get
.BI "(struct srpt_port *sp "  ","
.BI "struct ib_mad *rq_mad "  ","
.BI "struct ib_dm_mad *rsp_mad "  ");"
.SH ARGUMENTS
.IP "sp" 12
