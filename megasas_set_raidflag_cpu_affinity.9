.BI "bool fp_possible "  ","
.BI "u8 is_read "  ","
.BI "u32 scsi_buff_len "  ");"
.SH ARGUMENTS
.IP "fusion" 12
Fusion context
.IP "praid_context" 12
IO RAID context
.IP "raid" 12
LD raid map
